<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="389.0" prefWidth="426.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.coursemngsys.coursemanagementsystem.fxControllers.SelectedCourseWindow">
   <children>
      <Button fx:id="edit" layoutX="326.0" layoutY="21.0" mnemonicParsing="false" onAction="#editCourse" text="Edit Course" />
      <Button fx:id="delete" layoutX="313.0" layoutY="347.0" mnemonicParsing="false" onAction="#deleteCourse" text="DELETE COURSE" textFill="RED" />
      <TreeView fx:id="folderTree" layoutX="14.0" layoutY="14.0" prefHeight="358.0" prefWidth="289.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem mnemonicParsing="false" onAction="#addFolder" text="Add Folder" />
                  <MenuItem mnemonicParsing="false" text="Edit Folder" />
                  <MenuItem mnemonicParsing="false" text="Remove Folder" />
                  <MenuItem mnemonicParsing="false" onAction="#showFolderInfo" text="Show Folder Info" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TreeView>

   </children>
</AnchorPane>
